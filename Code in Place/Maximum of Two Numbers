#include<stdio.h>
int main()
{
	//Find Maximum of Two numbers
	int num1,num2;
	printf("Enter first number:\n");
	scanf("%d",&num1);
	
	printf("Enter second number:\n");
	scanf("%d",&num2);
	
	if(num1>=num2){
		printf("The first number is greater!");
	} else if(num1<=num2){
		printf("The Second number is greater!");
	} else if(num1==num2){
		printf("Both numbers are equal!");
	}
	

	return 0;
}
	
	
	
